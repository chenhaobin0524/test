{
  "title": {
    "text": "人员列表",
    "note": "备注信息，一般用来简单描述该列表页面的用途"
  },
  "toolbar": {
    "buttons": [
      {
        "label": "新建",
        "event": "on-create"
      },
      {
        "label": "导入",
        "event": "on-import",
        "btnFdKey": "btn-import",
        "menuFdKey": "mpanel"
      }
    ],
    "miniSearch": [
      {
        "type": "Input",
        "width": "120px",
        "label": "姓名",
        "field": "name",
        "labelWidth": 50
      },
      {
        "type": "Select",
        "width": "80px",
        "items": {
          "options": [
            {
              "label": "男",
              "id": 1
            },
            {
              "label": "女",
              "id": 0
            }
          ],
          "props": {
            "label": "label",
            "value": "id"
          }
        },
        "label": "性别",
        "field": "gender",
        "labelWidth": 50
      }
    ]
  },
  "table": {
    "index": true,
    "multiSelect": true,
    "columns": [
      {
        "title": "姓名",
        "key": "name",
        "align": "center"
      },
      {
        "title": "性别",
        "key": "gender",
        "align": "center"
      },
      {
        "title": "部门",
        "key": "apartment",
        "align": "center"
      }
    ],
    "page": {
      "showElevator": true,
      "showSizer": true,
      "pageSizeOpts": [5, 10, 20, 50, 100]
    },
    "action": [
      {
        "icon": "edit",
        "label": "修改",
        "event": "on-row-update",
        "btnFdKey": "btn-row-update",
        "menuFdKey": "mpanel"
      },
      {
        "icon": "eye",
        "label": "详情",
        "event": "on-row-details"
      },
      {
        "icon": "trash-b",
        "label": "删除",
        "event": "on-row-delete",
        "btnFdKey": "btn-row-delete",
        "menuFdKey": "mpanel"
      }
    ]
  }
}